<project name="get_libs" default="default" basedir=".">
	<description>
		Gets the minified JS files needed
	</description>
	
	<target name="default">
		<get src="https://cdnjs.cloudflare.com/ajax/libs/d3/4.13.0/d3.min.js"
			dest="d3.min.js" 
			skipexisting="true"	
		/>
		<get src="https://momentjs.com/downloads/moment.min.js"
			dest="moment.min.js"
			skipexisting="true"
		/>
		<get src="http://code.jquery.com/jquery-3.3.1.min.js"
			dest="jquery-3.3.1.min.js"
			skipexisting="true"
		/>
	</target>
</project>